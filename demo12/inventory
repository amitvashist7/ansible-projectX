web01 ansible_ssh_host=172.31.0.11 
db01 ansible_ssh_host=172.31.0.12 


[webservers]
web01

[dbservers]
db01

[mailservers]
web01

# Let's create a parent group as datacenter
[datacenter:children]
webservers
dbservers


# Let's creat a varible for group datacenter
[datacenter:vars]
ansible_ssh_user=vagrant
ansible_ssh_pass=vagrant
